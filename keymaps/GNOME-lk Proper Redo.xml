<keymap version="1" name="GNOME-lk Proper Redo" parent="Default for GNOME">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="Console.TableResult.NextPage" />
  <action id="Console.TableResult.PreviousPage" />
  <action id="EditorDeleteLine" />
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
  <action id="EditorUnSelectWord">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="GuiDesigner.ShrinkSelection">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
  </action>
  <action id="MoveLineDown">
    <keyboard-shortcut first-keystroke="shift ctrl down" />
    <keyboard-shortcut first-keystroke="ctrl alt down" />
  </action>
  <action id="MoveLineUp">
    <keyboard-shortcut first-keystroke="shift ctrl up" />
    <keyboard-shortcut first-keystroke="ctrl alt up" />
  </action>
  <action id="MoveStatementDown" />
  <action id="MoveStatementUp" />
  <action id="NextOccurence" />
  <action id="PreviousOccurence" />
  <action id="ResizeToolWindowDown" />
  <action id="ResizeToolWindowUp" />
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="SmartSelect">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
  <action id="SmartUnSelect">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
  </action>
  <action id="TableResult.GrowSelection">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
  <action id="TableResult.ShrinkSelection">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
  </action>
</keymap>